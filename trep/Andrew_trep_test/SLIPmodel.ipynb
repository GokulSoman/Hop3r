    "### normal stance phase system\n",
    "\n",
    "import trep\n",
    "from trep import tx,ty,tz,rx,ry,rz\n",
    "import time\n",
    "import trep.visual as visual\n",
    "import numpy as np\n",
    "import math\n",
    "import matplotlib.pyplot as plt\n",
    "%matplotlib inline\n",
    "\n",
    "dt = 0.01\n",
    "tf = 5.0\n",
    "\n",
    "spring_length = 2.5\n",
    "\n",
    "def simulate_system(system, force_func):\n",
    "    # Now we'll extract the current configuration into a tuple to use as\n",
    "    # initial conditions for a variational integrator.\n",
    "    q0 = system.q\n",
    "\n",
    "    # Create and initialize the variational integrator\n",
    "    mvi = trep.MidpointVI(system)\n",
    "    mvi.initialize_from_configs(0.0, q0, dt, q0)\n",
    "    \n",
    "#     print system.nQ\n",
    "#     print system.nQd\n",
    "#     print system.nQk\n",
    "    \n",
    "    # calculate Hamiltonian\n",
    "    p = [system.L_ddq(a) for a in system.configs]\n",
    "    qdot = system.dq\n",
    "        \n",
    "    H1 = np.dot(p, qdot) - system.L()\n",
    "    H = [H1]\n",
    "    \n",
    "    E1 = system.total_energy()\n",
    "    E = [E1]\n",
    "\n",
    "    # This is our simulation loop.  We save the results in two lists.\n",
    "    q = [mvi.q2]\n",
    "    t = [mvi.t2]\n",
    "    u = [0]\n",
    "    \n",
    "    while mvi.t1 < tf:\n",
    "#         mvi.step(mvi.t2+dt)\n",
    "        u.append(forcing(mvi.t2))\n",
    "        mvi.step(mvi.t2+dt, force_func(mvi.t2))\n",
    "        q.append(mvi.q2)\n",
    "        t.append(mvi.t2)\n",
    "        \n",
    "        # calculate Hamiltonian\n",
    "        p = [system.L_ddq(a) for a in system.configs]\n",
    "        qdot = system.dq\n",
    "\n",
    "        H1 = np.dot(p, qdot) - system.L()\n",
    "        \n",
    "        E1 = system.total_energy()\n",
    "\n",
    "#         print H1\n",
    "#         print '\\n'\n",
    "        \n",
    "        H.append(H1)\n",
    "        E.append(E1)\n",
    "\n",
    "    return (t,q,u,H,E)\n",
    "\n",
    "\n",
    "# system for stance\n",
    "stance = trep.System()\n",
    "stance.import_frames([\n",
    "    tz(0,name='foot'), [\n",
    "        rx('theta0'), [\n",
    "            tz('l',mass=1,name='hip')\n",
    "        ]\n",
    "    ]\n",
    "])\n",
    "\n",
    "print stance.nu\n",
    "\n",
    "# trep.potentials.LinearSpring(stance,'foot','hip',k=500,x0=spring_length)\n",
    "trep.forces.LinearDamper(stance,'foot','hip',c=0.0)\n",
    "# trep.potentials.Gravity(stance,(0,0,-9.8))\n",
    "\n",
    "# forcing function to simulate spring\n",
    "def forcing(t):\n",
    "    return tuple([0, 3*math.sin((t/5))])\n",
    "\n",
    "\n",
    "stance.q = [0.,2.]\n",
    "\n",
    "# Simulate\n",
    "start = time.clock()\n",
    "(t, q, H, E) = simulate_system(stance, forcing)\n",
    "finish = time.clock()\n",
    "\n",
    "# plot Hamiltonian\n",
    "plt.plot(t,E)\n",
    "plt.show()\n",
    "# for SLIPmodel, total energy is constant!\n",
    "\n",
    "# Display\n",
    "print \"Simulation: dt=%f, tf=%f, runtime=%f s\" % (dt, tf, finish-start)\n",
    "visual.visualize_3d([ visual.VisualItem3D(stance, t, q) ])"
   ]
